{
  "fieldName": "Existing b",
  "fieldId": "existingBICAndDN",
  "fieldType": "v6ExpandableRowTable",

  "expandBehavior": {
    "expandByDefaultIfSingleRow": true,
    "allowMultipleExpanded": true
  },

  /* ----------------------------------------------------
   * MASTER DATA SOURCE CONFIGURATION
   * ---------------------------------------------------- */
  "masterDataSource": {
    "type": "FORM",
    "formSource": {
      "fieldName": "accounts",
      "filter": {
        "field": "status",
        "operator": "EQUALS",
        "value": "OPEN"
      }
    }
  },

  /* ----------------------------------------------------
   * MASTER GRID HEADERS
   * ---------------------------------------------------- */
  "masterRowHeaders": [
    {
      "headerName": "Account Number",
      "field": "ACCT_NUMBER",
      "enableRowGroup": true,
      "sortable": true,
      "filter": true,
      "cellRenderer": "group"
    }
  ],

  /* ----------------------------------------------------
   * DETAIL GRID HEADERS
   * ---------------------------------------------------- */
  "detailRowHeaders": [
    {
      "headerName": "Process Type",
      "field": "PRCS_TYPE",
      "filter": true
    },
    {
      "headerName": "Address Type",
      "field": "ADRS_TYPE",
      "filter": true
    },
    {
      "headerName": "Address Data",
      "field": "ADRS_DATA",
      "filter": true
    }
  ],

  /* ----------------------------------------------------
   * DETAIL SERVICE CONFIGURATION
   * ---------------------------------------------------- */
  "service": {
    "clientIdentifier": "hg",
    "target": "DB2",
    "virtualTableName": "_ACCT",
    "columnName": "ACCOUNT",
    "columnValue": "AcctNumberArray",

    "selectColumns": [
      "PRCS_TYPE",
      "ADRS_TYPE",
      "ADRS_DATA"
    ],

    "method": "GET",
    "url": "/getCall",

    /* ------------------------------------------------
     * GENERIC RESPONSE CONTRACT
     * Backend always returns:
     * { tableName, values, childRows }
     * ------------------------------------------------ */
    "responseMapping": {
      "masterKeyField": "ACCT_NUMBER",
      "valuesField": "values",
      "childRowsField": "childRows"
    }
  }
}
